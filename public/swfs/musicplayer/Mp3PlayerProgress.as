package  {		import flash.display.MovieClip;			public class Mp3PlayerProgress extends MovieClip {				public var _paused:Boolean;		public var _clicked:Boolean;				public function Mp3PlayerProgress() {			// constructor code		}				public function get paused():Boolean		{			return _paused;		}				public function set paused(val:Boolean):void		{			_paused = val;		}				public function get clicked():Boolean		{			return _clicked;		}				public function set clicked(val:Boolean):void		{			_clicked = val;		}			}	}