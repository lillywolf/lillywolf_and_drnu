<html>
	<body>
		<style>
			#toplink_music {
				background-image: url("images/links/link_music_over.png");
				width: 113px;
				margin-left: -15px;
				margin-top: -4px;
				height: 24px;
			}		
		</style>
		<div id="songs">
		
			<% @songs.each do |song| %>
			<div class="song">
		
				<% @sc_swfurl = song.sc_swfurl_1 %>
		
				<div class="title"><%= link_to song.title, "http://soundcloud.com/lillywolf/" + song.url_ext %></a></div> 		
				<object height="81" width="100%"> <param name="movie" value=<%="#{@sc_swfurl}"%>></param> <param name="allowscriptaccess" value="always"></param> <embed allowscriptaccess="always" height="81" src=<%="#{@sc_swfurl}"%> type="application/x-shockwave-flash" width="100%"></embed> </object>	
		
		
				<div class="fb_like">
					
					<% @url_ext = "http://soundcloud.com/lillywolf/" + song.url_ext %>
					
					<div id="fb-root"></div>
			<script>
			(function(d, s, id) {
			  	var js, fjs = d.getElementsByTagName(s)[0];
			  	if (d.getElementById(id)) {
					// alert("return")
					// return;
				}
			  	js = d.createElement(s); 
				js.id = id;
			  	js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
			  	fjs.parentNode.insertBefore(js, fjs);	
			}
			(document, 'script', 'facebook-jssdk'));
			</script>

					<div class="fb-like" data-href=<%= "#{@url_ext}" %> data-colorscheme="dark" data-send="true" data-width="450" data-show-faces="false" data-font="verdana"></div>
				</div>
		
			</div>	
			<% end %>
			
		</div>	
	</body>
</html>	